				}
			}
			.Rollover
			{
				layer {
					LayerFlags  = GFlags(GCtrlBitmapSkin, GFC_BITMAPSKIN_CENTER_FIXED);
					DestCombine	= GColorCombine(BlendSource,Disable);
					Image		= GBitmap("Civ4/tab_arrows.tga", HVGrid, 4, 4, 3, 2);
					Margin		= GRectMargin(0,0,0,0);
				}
			}
			.Press
			{
				layer {
